{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["react-app", "react-app/jest", "naver", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks"],
  "rules": {
    "no-var": "error",
    "eqeqeq": "error",
    "require-await": "error",
    "no-unused-vars": "warn"
  }
}
